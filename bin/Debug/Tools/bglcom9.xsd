	<xs:attribute name="width" form="unqualified" type="stAngle" />
	<xs:attribute name="name" form="unqualified" type="stString48" />
	<xs:attribute name="backCourse" form="unqualified" type="stBool" />
</xs:complexType>

<xs:group name="grpIlsChildren">
