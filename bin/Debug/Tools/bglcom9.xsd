	<xs:attribute name="reil" form="unqualified" type="stBool" />
	<xs:attribute name="strobes" form="unqualified" type="xs:nonNegativeInteger" />
	<xs:attribute name="endLights" form="unqualified" type="stBool" />
	<xs:attribute name="touchdown" form="unqualified" type="stBool" />
</xs:complexType>

<xs:complexType name="ctBlastPad">
	<xs:attribute name="end" form="unqualified" type="stEnd" use="required" />
