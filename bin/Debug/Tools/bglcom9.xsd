<xs:annotation>
	<xs:documentation xml:lang="en">
	Schema for scenery and AFD data supported by facilities compiler.
	This file is: "bglcomp.xsd"
	</xs:documentation>
</xs:annotation>

<xs:element name="FSData" type="ctFSData" />


<!-- Complex Types -->

<xs:complexType name="ctFSData">
	<xs:sequence>
		<xs:group ref="grpFSDataChildren" minOccurs="0" maxOccurs="unbounded" />
	</xs:sequence>
	<xs:attribute name="version" form="unqualified" type="xs:string" />
</xs:complexType>

<xs:group name="grpFSDataChildren">
	<xs:choice>
		<xs:element name="Airport" type="ctAirport" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Marker" type="ctMarker" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Waypoint" type="ctWaypoint" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Boundary" type="ctBoundary" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Vor" type="ctVor" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Ndb" type="ctNdb" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="SceneryObject" type="ctSceneryObject" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="ModelData" type="ctModelData" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Geopol" type="ctGeopol" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="ExclusionRectangle" type="ctExclusionRectangle" minOccurs="0" maxOccurs="unbounded" />
	</xs:choice>
</xs:group>

<xs:complexType name="ctAirport">
	<xs:sequence>
		<xs:group ref="grpAirportChildren" minOccurs="0" maxOccurs="unbounded" />
	</xs:sequence>
	<xs:attribute name="country" form="unqualified" type="stString48" />
	<xs:attribute name="state" form="unqualified" type="stString48" />
	<xs:attribute name="city" form="unqualified" type="stString48" />
	<xs:attribute name="name" form="unqualified" type="stString48" />
	<xs:attribute name="region" form="unqualified" type="stString48" />
	<xs:attribute name="lat" form="unqualified" type="stLatitude" use="required" />
	<xs:attribute name="lon" form="unqualified" type="stLongitude" use="required"/>
	<xs:attribute name="alt" form="unqualified" type="stAltitude" use="required" />
	<xs:attribute name="magvar" form="unqualified" type="stAngle" />
	<xs:attribute name="ident" form="unqualified" type="stString5" use="required" />
</xs:complexType>

<xs:group name="grpAirportChildren">
	<xs:choice>
		<xs:element name="Tower" type="ctTower" minOccurs="0" maxOccurs="1" />
		<xs:element name="Services" type="ctServices" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Com" type="ctCom" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Runway" type="ctRunway" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="RunwayAlias" type="ctRunwayAlias" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Aprons" type="ctAprons" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="ApronEdgeLights" type="ctApronEdgeLights" minOccurs="0" maxOccurs="unbounded" />
        
        <!-- Taxiway data needs to be specifically ordered -->
        <xs:sequence>
    		<xs:element name="TaxiwayPoint" type="ctTaxiwayPoint" minOccurs="0" maxOccurs="unbounded" />
    		<xs:element name="TaxiwayParking" type="ctTaxiwayParking" minOccurs="0" maxOccurs="unbounded" />
    		<xs:element name="TaxiName" type="ctTaxiName" minOccurs="0" maxOccurs="unbounded" />
    		<xs:element name="TaxiwayPath" type="ctTaxiwayPath" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        
		<xs:element name="TaxiwaySign" type="ctTaxiwaySign" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Waypoint" type="ctWaypoint" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Approach" type="ctApproach" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Ndb" type="ctNdb" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Helipad" type="ctHelipad" minOccurs="0" maxOccurs="unbounded" />
		<xs:element name="Start" type="ctStart" minOccurs="0" maxOccurs="unbounded" />
        <xs:element name="DeleteAirport" type="ctDeleteAirport" minOccurs="0" maxOccurs="unbounded" />
	</xs:choice>
</xs:group>

<xs:complexType name="ctDeleteAirport">
    <xs:sequence> 
        <xs:group ref="grpDeleteAirportChildren" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:attribute name="deleteAllApproaches" form="unqualified" type="stBool" />
    <xs:attribute name="deleteAllApronLights" form="unqualified" type="stBool" />
    <xs:attribute name="deleteAllAprons" form="unqualified" type="stBool" />
    <xs:attribute name="deleteAllFrequencies" form="unqualified" type="stBool" />
    <xs:attribute name="deleteAllHelipads" form="unqualified" type="stBool" />
    <xs:attribute name="deleteAllRunways" form="unqualified" type="stBool" />
    <xs:attribute name="deleteAllStarts" form="unqualified" type="stBool" />
    <xs:attribute name="deleteAllTaxiways" form="unqualified" type="stBool" />
</xs:complexType>

<xs:group name="grpDeleteAirportChildren">
    <xs:choice>
        <xs:element name="DeleteRunway" type="ctDeleteRunway" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="DeleteStart" type="ctDeleteStart" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="DeleteFrequency" type="ctDeleteFrequency" minOccurs="0" maxOccurs="unbounded"/>
    </xs:choice>
</xs:group>

<xs:complexType name="ctDeleteRunway">
    <xs:attribute name="surface" form="unqualified" type="stSurface" use="required" />
    <xs:attribute name="number" form="unqualified" type="stRunwayNumber" use="required" />
    <xs:attribute name="designator" form="unqualified" type="stDesignator" />
</xs:complexType>

<xs:complexType name="ctDeleteStart">
    <xs:attribute name="type" form="unqualified" type="stStart" use="required" />
    <xs:attribute name="number" form="unqualified" type="stRunwayNumber" use="required" />
    <xs:attribute name="designator" form="unqualified" type="stDesignator" />
</xs:complexType>

<xs:complexType name="ctDeleteFrequency">
    <xs:attribute name="frequency" form="unqualified" type="stFrequencyMhz" use="required" />
    <xs:attribute name="type" form="unqualified" type="stComType" use="required" />
</xs:complexType>

<xs:complexType name="ctMarker">
	<xs:attribute name="lat" form="unqualified" type="stLatitude" />
	<xs:attribute name="lon" form="unqualified" type="stLongitude" />
	<xs:attribute name="alt" form="unqualified" type="stAltitude" />
	<xs:attribute name="type" form="unqualified" type="stMarkerType" />
	<xs:attribute name="heading" form="unqualified" type="stHeading" />
	<xs:attribute name="ident" form="unqualified" type="stString5" />
	<xs:attribute name="region" form="unqualified" type="stString2" />
	<xs:attribute name="name" form="unqualified" type="stString48" />
</xs:complexType>

<xs:complexType name="ctCom">
	<xs:attribute name="frequency" form="unqualified" type="stFrequencyMhz" />
	<xs:attribute name="type" form="unqualified" type="stComType" />
	<xs:attribute name="name" form="unqualified" type="stString48" />
</xs:complexType>

<xs:complexType name="ctOffsetThreshold">
	<xs:attribute name="end" form="unqualified" type="stEnd" use="required" />
	<xs:attribute name="length" form="unqualified" type="stDistance" use="required" />
	<xs:attribute name="width" form="unqualified" type="stDistance" />
	<xs:attribute name="surface" form="unqualified" type="stSurface" />
</xs:complexType>

<xs:complexType name="ctRunwayStart">
	<xs:attribute name="end" form="unqualified" type="stEnd" />
	<xs:attribute name="lat" form="unqualified" type="stLatitude" use="required" />
	<xs:attribute name="lon" form="unqualified" type="stLongitude" use="required" />
	<xs:attribute name="alt" form="unqualified" type="stAltitude" use="required" />
	<xs:attribute name="heading" form="unqualified" type="stHeading" use="required" />
	<xs:attribute name="type" form="unqualified" type="stStart" />
</xs:complexType>

<xs:complexType name="ctStart">
	<xs:attribute name="number" form="unqualified" type="stRunwayNumber" />
	<xs:attribute name="designator" form="unqualified" type="stDesignator" />
	<xs:attribute name="lat" form="unqualified" type="stLatitude" use="required" />
	<xs:attribute name="lon" form="unqualified" type="stLongitude" use="required" />
	<xs:attribute name="alt" form="unqualified" type="stAltitude" use="required" />
	<xs:attribute name="heading" form="unqualified" type="stHeading" use="required" />
	<xs:attribute name="type" form="unqualified" type="stStart" />
</xs:complexType>


<xs:complexType name="ctMarkings">
	<xs:attribute name="edges" form="unqualified" type="stBool" />
	<xs:attribute name="threshold" form="unqualified" type="stBool" />
